<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hjf.dao.SsoPermissionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hjf.entity.SsoPermission">
        <id column="permission_code" property="permissionCode" />
        <result column="url" property="url" />
        <result column="status" property="status" />
        <result column="description" property="description" />
        <result column="create_by" property="createBy" />
        <result column="create_date" property="createDate" />
        <result column="update_dy" property="updateDy" />
        <result column="update_date" property="updateDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        permission_code, url, status, description, create_by, create_date, update_dy, update_date
    </sql>

</mapper>
